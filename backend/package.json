{
  "name": "guess5-backend",
  "version": "1.0.0",
  "description": "Backend for Guess5 game",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build completed successfully'",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/db/index.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/db/index.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/db/index.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/db/index.ts"
  },
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "@types/ws": "^8.5.10",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
